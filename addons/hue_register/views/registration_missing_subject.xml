<odoo>
	<data>
		<!--Registration Missing Subject Tree-->
		<record model="ir.ui.view" id="hue_student_registration_missing_subject_tree">
			<field name="name">Registration Missing Subject Tree</field>
			<field name="model">hue.registration.missing.subj</field>
			<field name="arch" type="xml">
				<tree string="Registration Missing Subject">
					<field name="id"/>
					<field name="create_date"/>
				</tree>
			</field>
		</record>
		<!--Actions-->
		<record id="hue_student_registration_missing_subject_action" model="ir.actions.act_window">
			<field name="name">Registration Missing Subject</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">hue.registration.missing.subj</field>
			<field name="view_mode">tree,form</field>
		</record>
	</data>
</odoo>