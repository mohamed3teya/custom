<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<!--intern_department_tree_view-->
		<record id="intern_department_tree_view" model="ir.ui.view">
			<field name="name">training places</field>
			<field name="model">hue.intern.department</field>
			<field name="arch" type="xml">
				<tree string="Intern Department">
					<field name="course_id"/>
					<field name="name"/>
					<field name="en_name"/>
				</tree>
			</field>
		</record>
		<!--intern_department_form_view-->
		<record id="intern_department_form_view" model="ir.ui.view">
			<field name="name">hue intern department</field>
			<field name="model">hue.intern.department</field>
			<field name="arch" type="xml">
				<form string="Intern Department" >
					<sheet>
						<group col="4">
							<field name="course_id"/>
							<field name="name"/>
							<field name="en_name"/>
						</group>
					</sheet>
					<div class="oe_chatter">
						<field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
						<field name="message_ids" widget="mail_thread"/>
					</div>
				</form>
			</field>
		</record>
		<!--graduation_project_action-->
		<record model="ir.actions.act_window" id="intern_department_action">
			<field name="name">Intern Department</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">hue.intern.department</field>
			<field name="view_mode">tree,form</field>
		</record>
	</data>
</odoo>