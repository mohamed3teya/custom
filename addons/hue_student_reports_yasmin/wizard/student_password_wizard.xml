<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- form -->
        <record id="student_password_wizard_form" model="ir.ui.view">
            <field name="name">student password  form</field>
            <field name="model">student.password.wizard</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="student password Report">
                    <sheet>
                        <div>
                            <group colspan="8" cols="8">
                                <group>
                                <field name="student_id"  />
                                </group>
                            </group>
                        </div>
                    </sheet>
                    <footer>
                        <span groups="base.group_user">
                            <button string="Print" type="object" name="print_report" class="oe_highlight"/>
                            <button special="cancel" string="Close" type="object"/>
                        </span>
                    </footer>
                </form>
            </field>
        </record>
        <!-- action  -->
        <record model="ir.actions.act_window" id="action_student_password_wizard">
            <field name="name">student password  Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">student.password.wizard</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <!-- menu -->
         <!-- <menuitem id="menu_student_password_wizard"
                  name="student_password Report"
                  sequence="2"
                  parent="hue_student_reports_yasmin.submenu_student_data_report"
                  action="action_student_password_wizard"/> -->
    </data>
</odoo>
