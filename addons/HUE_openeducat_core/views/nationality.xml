<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
		<!--view_hue_nationalities_form_view-->
		<record id="view_hue_certificates_form_view" model="ir.ui.view">
			<field name="name">hue.nationalities.form.view</field>
			<field name="model">hue.nationalities</field>
			<field name="arch" type="xml">
				<form string="Nationality">
					<sheet>
						<group>
							<group>
								<field name="name"/>
								<field name="en_name"/>
							</group>
							<group>
								<field name="d_id"/>
								<field name="foreign_nationality"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>
		<!--act_open_op_nationality-->
		<record model="ir.actions.act_window" id="act_open_op_nationality">
			<field name="name">Nationalities Types</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">hue.nationalities</field>
			<field name="view_mode">tree,form</field>
		</record>
		<!-- get_hue_nationalities_action -->
		<record id="get_hue_nationalities_action" model="ir.actions.server">
			<field name="name">Get Hue Nationalities menu</field>
			<field name="model_id" ref="model_hue_nationalities"/>
			<field name="state">code</field>
			<field name="code">model.get_nationalities_fun()</field>
		</record>
	</data>
</odoo>